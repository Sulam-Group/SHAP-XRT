{
    "train_batch_size": 64,
    "gradient_accumulation_steps": 2,
    "optimizer": {
        "type": "Adam",
        "params": {
            "lr": 2e-04,
            "betas": [
                0.9,
                0.999
            ]
        },
        "eps": 1e-08
    },
    "gradient_clipping": 1.0,
    "scheduler": {
        "type": "WarmupLR",
        "params": {
            "warmup_min_lr": 0,
            "warmup_max_lr": 2e-04,
            "warmup_num_steps": 5000
        }
    },
    "wandb": {
        "enabled": true,
        "team": "jacopoteneggi",
        "project": "CelebAMask-HQ"
    }
}